{
    "version": 2,
    "builds": [
        {
            "src":"*.js",
            "use":"@vercel/node"
        }
    ],
    "routes": [
      {
        "src": "/models",
        "methods": ["GET"],
        "dest": "/models",
        "cors": {
          "origin": "*",
          "methods": ["GET"],
          "headers": ["Content-Type"]
        }
      },
      {
        "src": "/postModel",
        "methods": ["POST"],
        "dest": "/postModel",
        "cors": {
          "origin": "*",
          "methods": ["POST"],
          "headers": ["Content-Type"]
        }
      },
      {
        "src": "/",
        "methods": ["POST"],
        "dest": "/",
        "cors": {
          "origin": "*",
          "methods": ["POST"],
          "headers": ["Content-Type"]
        }
      },
      {
        "src": "/postSlider",
        "methods": ["POST"],
        "dest": "/postSlider",
        "cors": {
          "origin": "*",
          "methods": ["POST"],
          "headers": ["Content-Type"]
        }
      }
    ]
  }